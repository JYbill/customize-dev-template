  "imports": {
    "#app/*": "./src/*",
    "#utils/*": "./src/utils/*",
    "#lib/*": "./src/library/*",
    "#db/*": "./src/library/kysely/*",
    "#db": "./src/library/kysely/index.ts",
    "#error/*": "./src/utils/error/*",
    "#middleware/*": "./src/middleware/*",
    "#service/*": "./src/service/*",
    "#config": "./src/config/index.ts",
    "#enum/*": "./src/enum/*",
    "#logger": "./src/library/logger/index.ts",
    "#ws/*": "./src/ws/*",
    "#types/*": "./src/types/*"
  },
  "scripts": {
    "prepare": "husky",
    "format": "prettier --write src",
    "lint": "eslint --ext ts,cts,mts --fix src",
    "lint-staged": "lint-staged",
    "start:prod": "cross-env pm2-runtime pm2.config.cjs",
    "dev:debug": "cross-env NODE_ENV=development NODE_OPTIONS='--inspect' node --watch src/index.ts",
    "dev": "cross-env NODE_ENV=development node --watch src/index.ts",
    "dev:local": "cross-env NODE_ENV=local node --watch src/index.ts",
    "deploy:docker": "bash ./docker-build.sh -p x86-debian"
